{
	"version": "1",
	"id": "japanese-minimalist",
	"name": "Japanese Minimalist",
	"tags": [
		"minimal",
		"calm",
		"balanced",
		"light"
	],
	"aliases": [
		"japanese",
		"zen"
	],
	"daisyThemeVars": {
		"--color-base-100": "oklch(99% 0.01 240)",
		"--color-base-200": "oklch(96% 0.01 240)",
		"--color-base-300": "oklch(92% 0.01 240)",
		"--color-base-content": "oklch(18% 0.02 240)",
		"--color-primary": "oklch(55% 0.06 20)",
		"--color-primary-content": "oklch(98% 0.01 20)",
		"--color-secondary": "oklch(60% 0.06 170)",
		"--color-secondary-content": "oklch(98% 0.01 170)",
		"--color-accent": "oklch(70% 0.08 80)",
		"--color-accent-content": "oklch(18% 0.02 80)",
		"--color-neutral": "oklch(35% 0.02 240)",
		"--color-neutral-content": "oklch(96% 0.01 240)",
		"--color-info": "oklch(68% 0.10 230)",
		"--color-info-content": "oklch(14% 0.02 230)",
		"--color-success": "oklch(64% 0.12 150)",
		"--color-success-content": "oklch(98% 0.01 150)",
		"--color-warning": "oklch(80% 0.14 85)",
		"--color-warning-content": "oklch(22% 0.04 85)",
		"--color-error": "oklch(58% 0.20 25)",
		"--color-error-content": "oklch(98% 0.01 25)",
		"--radius-selector": "0.25rem",
		"--radius-field": "0.25rem",
		"--radius-box": "0.5rem",
		"--size-selector": "0.25rem",
		"--size-field": "0.25rem",
		"--border": "1px",
		"--depth": "0",
		"--noise": "0"
	},
	"metadata": {
		"description": "Calm, minimal aesthetic with gentle accents.",
		"builtInThemeHint": "lofi"
	},
	"extended": {
		"typography": {
			"heading": "light-elegant",
			"body": "widely-spaced",
			"fontStacks": {
				"display": {
					"fontsource": {
						"package": "@fontsource/noto-sans-jp",
						"family": "Noto Sans JP",
						"weights": [
							300,
							400,
							500,
							700,
							900
						],
						"styles": [
							"normal"
						],
						"variable": true
					},
					"fallbacks": [
						"Hiragino Sans",
						"Yu Gothic",
						"Meiryo",
						"sans-serif"
					],
					"css": "'Noto Sans JP', 'Hiragino Sans', 'Yu Gothic', Meiryo, sans-serif",
					"performance": {
						"fontDisplay": "swap",
						"preload": true,
						"priority": "high"
					}
				},
				"body": {
					"fontsource": {
						"package": "@fontsource/source-serif-pro",
						"family": "Source Serif Pro",
						"weights": [
							300,
							400,
							600,
							700
						],
						"styles": [
							"normal",
							"italic"
						],
						"variable": true
					},
					"fallbacks": [
						"Georgia",
						"Times New Roman",
						"Times",
						"serif"
					],
					"css": "'Source Serif Pro', Georgia, 'Times New Roman', Times, serif",
					"performance": {
						"fontDisplay": "swap",
						"preload": false,
						"priority": "medium"
					}
				},
				"mono": {
					"fontsource": {
						"package": "@fontsource/noto-sans-mono",
						"family": "Noto Sans Mono",
						"weights": [
							300,
							400,
							500,
							600,
							700
						],
						"styles": [
							"normal"
						],
						"variable": true
					},
					"fallbacks": [
						"SF Mono",
						"Monaco",
						"Consolas",
						"monospace"
					],
					"css": "'Noto Sans Mono', 'SF Mono', Monaco, Consolas, monospace",
					"performance": {
						"fontDisplay": "swap",
						"preload": false,
						"priority": "low"
					}
				},
				"refined": {
					"fontsource": {
						"package": "@fontsource/noto-sans-jp",
						"family": "Noto Sans JP",
						"weights": [
							300,
							400,
							500
						],
						"styles": [
							"normal"
						],
						"variable": true
					},
					"fallbacks": [
						"Hiragino Sans",
						"Yu Gothic",
						"Meiryo",
						"sans-serif"
					],
					"css": "'Noto Sans JP', 'Hiragino Sans', 'Yu Gothic', Meiryo, sans-serif",
					"performance": {
						"fontDisplay": "swap",
						"preload": false,
						"priority": "medium"
					}
				}
			}
		},
		"zen": {
			"spacing": {
				"generous": "2rem",
				"breath": "3rem",
				"meditation": "4rem"
			},
			"balance": {
				"golden": "1.618",
				"asymmetric": "purposeful imbalance for visual interest"
			}
		},
		"naturalTones": {
			"offWhite": "#FEFEFE",
			"beige": "#F5F5DC",
			"lightBrown": "#D2B48C",
			"softGrey": "#F8F8FF",
			"subtleGreen": "#F0FFF0",
			"palette": [
				"#FEFEFE",
				"#F5F5DC",
				"#D2B48C",
				"#F8F8FF",
				"#F0FFF0",
				"#E6E6FA"
			],
			"earth": [
				"#DEB887",
				"#D2B48C",
				"#BC9A6A",
				"#A0956B"
			]
		},
		"minimalism": {
			"negativeSpace": "embrace emptiness as design element",
			"subtlety": "avoid sharp contrasts",
			"harmony": "colors blend seamlessly"
		},
		"interactions": {
			"gentle": {
				"hover": "opacity: 0.8",
				"focus": "outline: 1px solid rgba(0,0,0,0.1)",
				"active": "transform: translateY(1px)"
			},
			"buttons": {
				"minimal": "border: 1px solid; background: transparent",
				"underlined": "border-bottom: 1px solid"
			}
		},
		"animations": {
			"gentle-fade": "opacity 0.4s ease",
			"natural-flow": "all 0.6s cubic-bezier(0.25, 0.46, 0.45, 0.94)",
			"zen-breathe": "transform 4s ease-in-out infinite"
		}
	},
	"notes": "Embrace negative space. Use natural tones. Avoid sharp contrasts. Focus on balance and harmony."
}