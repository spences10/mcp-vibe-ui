{
	"version": "1",
	"id": "neumorphic",
	"name": "Neumorphic",
	"tags": [
		"soft",
		"shadowed",
		"embossed",
		"light"
	],
	"aliases": [
		"neumorphism"
	],
	"daisyThemeVars": {
		"--color-base-100": "oklch(98% 0.01 240)",
		"--color-base-200": "oklch(95% 0.01 240)",
		"--color-base-300": "oklch(90% 0.01 240)",
		"--color-base-content": "oklch(22% 0.02 240)",
		"--color-primary": "oklch(70% 0.06 210)",
		"--color-primary-content": "oklch(14% 0.02 210)",
		"--color-secondary": "oklch(76% 0.06 150)",
		"--color-secondary-content": "oklch(14% 0.02 150)",
		"--color-accent": "oklch(84% 0.06 90)",
		"--color-accent-content": "oklch(16% 0.02 90)",
		"--color-neutral": "oklch(40% 0.01 240)",
		"--color-neutral-content": "oklch(96% 0.01 240)",
		"--color-info": "oklch(72% 0.08 230)",
		"--color-info-content": "oklch(14% 0.02 230)",
		"--color-success": "oklch(68% 0.10 150)",
		"--color-success-content": "oklch(98% 0.01 150)",
		"--color-warning": "oklch(84% 0.12 85)",
		"--color-warning-content": "oklch(22% 0.04 85)",
		"--color-error": "oklch(62% 0.16 25)",
		"--color-error-content": "oklch(98% 0.01 25)",
		"--radius-selector": "1rem",
		"--radius-field": "1rem",
		"--radius-box": "1.5rem",
		"--size-selector": "0.25rem",
		"--size-field": "0.25rem",
		"--border": "1px",
		"--depth": "1",
		"--noise": "0"
	},
	"metadata": {
		"description": "Soft, tactile surfaces with subtle inner/outer shadows.",
		"builtInThemeHint": "cupcake"
	},
	"extended": {
		"typography": {
			"heading": "normal",
			"body": "normal",
			"fontStacks": {
				"display": {
					"fontsource": {
						"package": "@fontsource/inter",
						"family": "Inter",
						"weights": [
							400,
							500,
							600,
							700,
							800
						],
						"styles": [
							"normal",
							"italic"
						],
						"variable": true
					},
					"fallbacks": [
						"SF Pro Display",
						"system-ui",
						"-apple-system",
						"BlinkMacSystemFont",
						"sans-serif"
					],
					"css": "'Inter', 'SF Pro Display', system-ui, -apple-system, BlinkMacSystemFont, sans-serif",
					"performance": {
						"fontDisplay": "swap",
						"preload": true,
						"priority": "high"
					}
				},
				"body": {
					"fontsource": {
						"package": "@fontsource/inter",
						"family": "Inter",
						"weights": [
							300,
							400,
							500,
							600
						],
						"styles": [
							"normal",
							"italic"
						],
						"variable": true
					},
					"fallbacks": [
						"SF Pro Text",
						"system-ui",
						"-apple-system",
						"BlinkMacSystemFont",
						"sans-serif"
					],
					"css": "'Inter', 'SF Pro Text', system-ui, -apple-system, BlinkMacSystemFont, sans-serif",
					"performance": {
						"fontDisplay": "swap",
						"preload": false,
						"priority": "medium"
					}
				},
				"mono": {
					"fontsource": {
						"package": "@fontsource/sf-mono",
						"family": "SF Mono",
						"weights": [
							400,
							500,
							600,
							700
						],
						"styles": [
							"normal",
							"italic"
						],
						"variable": false
					},
					"fallbacks": [
						"Monaco",
						"Consolas",
						"monospace"
					],
					"css": "'SF Mono', Monaco, Consolas, monospace",
					"performance": {
						"fontDisplay": "swap",
						"preload": false,
						"priority": "low"
					}
				}
			}
		},
		"effects": {
			"shadows": {
				"outer": "8px 8px 16px rgba(0,0,0,0.1), -8px -8px 16px rgba(255,255,255,0.8)",
				"inner": "inset 4px 4px 8px rgba(0,0,0,0.1), inset -4px -4px 8px rgba(255,255,255,0.8)",
				"pressed": "inset 2px 2px 4px rgba(0,0,0,0.2), inset -2px -2px 4px rgba(255,255,255,0.9)"
			},
			"highlights": {
				"top": "rgba(255,255,255,0.9)",
				"bottom": "rgba(0,0,0,0.1)"
			}
		},
		"softColors": {
			"backgrounds": [
				"#f0f0f3",
				"#e6e6ea",
				"#f5f5f7",
				"#ebebef"
			],
			"raised": [
				"#f8f8fb",
				"#f2f2f5",
				"#fafafa"
			],
			"pressed": [
				"#e8e8eb",
				"#dedee2",
				"#e0e0e4"
			]
		},
		"animations": {
			"soft-press": "soft-press 0.15s ease-out",
			"soft-release": "soft-release 0.15s ease-out",
			"gentle-hover": "gentle-hover 0.3s ease-out"
		},
		"layout": {
			"borderRadius": {
				"soft": "12px",
				"medium": "16px",
				"large": "24px"
			},
			"spacing": {
				"tight": "8px",
				"normal": "16px",
				"loose": "24px"
			}
		}
	},
	"notes": "Best on light backgrounds. Ensure sufficient contrast for accessibility."
}