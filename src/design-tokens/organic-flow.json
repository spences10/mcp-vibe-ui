	"fonts": [
		{
			"package": "@fontsource/comfortaa",
			"weights": [300, 400, 500, 600, 700],
			"variable": true
		},
		{
			"package": "@fontsource/source-sans-pro",
			"weights": [300, 400, 600, 700],
			"variable": false
		},
		{
			"package": "@fontsource/source-code-pro",
			"weights": [300, 400, 500, 600, 700],
			"variable": true
		}
	],
	"metadata": {
		"description": "Organic rounded shapes with nature-inspired palette."
	},
	"extended": {
		"effects": {
			"shadow": "0 4px 20px rgba(34, 197, 94, 0.2)",
			"glow": "0 0 30px rgba(34, 197, 94, 0.3)",
			"blur": null
		},
		"animations": {
			"enter": "grow 0.6s cubic-bezier(0.25, 0.46, 0.45, 0.94)",
			"hover": "bloom 0.8s cubic-bezier(0.68, -0.55, 0.265, 1.55)",
			"press": null
		},
		"keyframes": {
			"grow": "0% { transform: scale(0.9); opacity: 0; }\n100% { transform: scale(1); opacity: 1; }",
			"sway": "0%, 100% { transform: rotate(-1deg); }\n50% { transform: rotate(1deg); }",
			"bloom": "0% { transform: scale(1); }\n50% { transform: scale(1.05); }\n100% { transform: scale(1); }"
		},
		"surfaces": {
			"raised": "shadow-[0_8px_30px_rgba(34,197,94,0.3)] rounded-2xl",
			"sunken": "shadow-inner bg-border/50",
			"overlay": "bg-background/90 backdrop-blur-sm rounded-3xl"
		},
		"themeSpecific": {
			"naturalColors": {
				"earth": ["#8B7355", "#A0956B", "#C4B5A0", "#D4C4AA"],
				"growth": ["#22c55e", "#16a34a", "#15803d", "#166534"],
				"sky": ["#87CEEB", "#B0E0E6", "#E0F6FF", "#F0F8FF"],
				"gradients": [
					"linear-gradient(135deg, #22c55e, #16a34a, #15803d)",
					"radial-gradient(circle, #87CEEB, #22c55e, #8B7355)"
				]
			},
			"organicShapes": {
				"blob1": "60% 40% 30% 70% / 60% 30% 70% 40%",
				"blob2": "30% 60% 70% 40% / 50% 60% 30% 60%",
				"blob3": "40% 60% 60% 40% / 60% 30% 60% 40%"
			},
			"flowingSpacing": {
				"rhythm": ["8px", "13px", "21px", "34px", "55px"],
				"golden": "1.618"
			}
		}
	},
	"notes": "Use organic animations subtly. Consider environmental impact in color choices."
}
